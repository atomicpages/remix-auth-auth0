{
  "name": "@djthoms/remix-auth-auth0",
  "version": "1.10.0",
  "keywords": [
    "remix",
    "remix-auth",
    "auth",
    "authentication",
    "strategy",
    "auth0"
  ],
  "homepage": "https://github.com/danestves/remix-auth-auth0#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/danestves/remix-auth-auth0"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./build/index.d.ts",
        "default": "./build/index.js"
      },
      "require": {
        "types": "./build/index.d.cts",
        "default": "./build/index.cjs"
      }
    }
  },
  "main": "./build/index.cjs",
  "module": "./build/index.js",
  "types": "./build/index.d.cts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint --ext .ts,.tsx src/",
    "prepare": "is-ci || husky",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "remix-auth-oauth2": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@djthoms/eslint-config": "^6.4.2",
    "@djthoms/prettier-config": "^4.4.1",
    "@remix-run/node": "^2.10.3",
    "@remix-run/react": "^2.10.3",
    "@remix-run/server-runtime": "^2.10.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitest/coverage-v8": "^2.0.3",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.7",
    "semantic-release": "^23.1.1",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3",
    "vitest-fetch-mock": "^0.3.0"
  },
  "peerDependencies": {
    "@remix-run/server-runtime": "^1.0.0 || ^2.0.0",
    "remix-auth": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
